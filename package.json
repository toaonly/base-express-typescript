{
  "name": "node_ts",
  "version": "1.0.0",
  "description": "nodejs base typescript",
  "scripts": {
    "build:prod": "webpack --config build/webpack.prod.conf.js --mode=production",
    "build:dev": "webpack --config build/webpack.dev.conf.js --mode=development",
    "watch:prod": "webpack --config build/webpack.prod.conf.js -w --mode=production",
    "watch:dev": "webpack --config build/webpack.dev.conf.js -w --mode=development",
    "start": "node dist/app",
    "start:prod": "npm run watch:prod",
    "start:dev": "npm run watch:dev",
    "run": "nodemon dist/app -w dist",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.0.0",
    "@types/supertest": "^2.0.7",
    "dotenv": "^7.0.0",
    "dotenv-webpack": "^1.7.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.11",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
