{
  "name": "node_ts",
  "version": "1.0.0",
  "description": "nodejs base typescript",
  "scripts": {
    "build:prod": "webpack --config build/webpack.prod.conf.js --mode=production",
    "build:dev": "webpack --config build/webpack.dev.conf.js --mode=development",
    "watch:prod": "webpack --config build/webpack.prod.conf.js -w --mode=production",
    "watch:dev": "webpack --config build/webpack.dev.conf.js -w --mode=development",
    "start": "node dist/app",
    "start:prod": "npm run watch:prod",
    "start:dev": "npm run watch:dev",
    "run": "nodemon dist/app -w dist",
    "test": "jest"
  },
  "keywords": [],
